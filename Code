package project1;

import java.util.Random;
import java.util.Scanner;

public class strt {

	public static void main(String[] args) {
		// distance between two points |
		System.out.println("Input two coordinate:");
		Scanner sc = new Scanner(System.in);
		String scnd = sc.next();
		Scanner sc2 = new Scanner(System.in);
		String scnd2 = sc2.next();
        //(x,y)
       //x = 1,y = 3
       String xa = scnd.substring(1,2);
       String ya = scnd.substring(3,4);
       Double x = Double.parseDouble(xa);
       Double y = Double.parseDouble(ya);
       //System.out.println(x + "," + y);
       String xa2 = scnd2.substring(1,2);
       String ya2 = scnd2.substring(3,4);
       Double x2 = Double.parseDouble(xa2);
       Double y2 = Double.parseDouble(ya2);


       
       Double d = Math.sqrt(Math.pow(x2-x,2) + Math.pow(y2-y,2));
       System.out.println("The distance between the two points is: " + d);
      
		
		
		
		
	}	
	}
	

